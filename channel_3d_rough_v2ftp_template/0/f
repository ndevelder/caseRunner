FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      f;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 0 -1 0 0 0 0];
internalField   uniform 1e-15; 
boundaryField
{
    WALL_TOP
    {
        type            fLowReRoughWallTP;
        ks              <ksValueExpression>;
		grex            0.1;
		Cf              1.1;
        value           uniform 0.0;
    }
    WALL_BOTTOM
    {
        type            fLowReRoughWallTP;
        ks              <ksValueExpression>;
		grex            0.1;
		Cf              1.1;
        value           uniform 0.0;
    }
    SIDE_CYC
    {
        type            cyclic;
		value           uniform 1e-15;
    }
    INOUT_CYC
    {
        type            cyclic;
        value           uniform 1e-15; 
    }
}
