FoamFile
{
    version 2.0;
    format ascii;
    class dictionary;
    object sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


setFormat raw;

surfaceFormat raw;

interpolationScheme cellPoint;

// Fields to sample.
fields
(
    U
    p
    k
    epsilon
    tpphi
    tppsi
    vorticity
    nut
);

sets
(
    lineY
    {
        type        uniform;
        axis        y;

        //- cavity. Slightly perturbed so not to align with face or edge.
        start       (0.01 1.0 0.0);
        end         (0.01 -1.0 0.0);
        nPoints     256;
    }
);

surfaces
(
);


