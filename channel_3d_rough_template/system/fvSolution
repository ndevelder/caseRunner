FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(p|pFinal)"
    {
        solver           PCG;
        preconditioner   FDIC;
        tolerance        1e-11;
        relTol           0.0000001;
        maxIter          184;
    };
    "(phis|phisFinal)"
    {
     	solver           PCG;
        preconditioner   DIC;
        tolerance        1e-12;
        relTol           0.0;
        minIter          12;
    };
    "(tpphi|tppsi|R)"
    {
        solver           smoothSolver;
        smoother	 GaussSeidel;
        tolerance        1e-11;
        relTol           0.00;
        maxIter          100;
        minIter          1;
    };
    "(k|epsilon)"
    {
        solver           smoothSolver;
        smoother         GaussSeidel;
        tolerance        1e-11;
        relTol           0.00;
        maxIter          100;
        minIter          1;
    };
    "(U|UFinal)"
    {
        solver           smoothSolver;
        smoother         symGaussSeidel;
        tolerance        1e-11;
        relTol           0.00;
        maxIter          100;
    };
}

PIMPLE
{
    nOuterCorrectors    6;
    nCorrectors         2;
    nNonOrthogonalCorrectors 1;
    pRefCell          101;
    pRefValue 1e-14;
    turbOnFinalIterOnly = true;    
    correctPhi no;
}

relaxationFactors
{
    p               0.5;
    U               0.7;
    k               0.6;
    epsilon         0.6;
    R               0.8;
    tpphi           0.5;
    tppsi           0.5;
    phis            0.5;
}

cache
{
grad(U);    
}

