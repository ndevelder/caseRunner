FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      kSqrt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 1 -1 0 0 0 0];
internalField   uniform 1.414; 
boundaryField
{
    WALL_TOP
    {
        type            fixedValue;
        value           uniform 0.548;
    }
    WALL_BOTTOM
    {
        type            fixedValue;
        value           uniform 0.548;
    }
    SIDE_CYC
    {
        type            cyclic;
        value           uniform 1.414;
    }
    INOUT_CYC
    {
        type            cyclic;
        value           uniform 1.414; 
    }
}
