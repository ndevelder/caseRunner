FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      tpphi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 0 0 0 0 0 0];
internalField   uniform 0.67;
boundaryField
{
    WALL_TOP
    {
        type            tpphiLowReRoughWallTP;
        ks              <ksValueExpression>;
        rType			<rTypeValue>;
        pkC             <pkCValue>;
        value           uniform 0.25;
    }
    WALL_BOTTOM
    {
        type            tpphiLowReRoughWallTP;
        ks              <ksValueExpression>;
		rType			<rTypeValue>;
		pkC             <pkCValue>;
        value           uniform 0.25;
    }
    SIDE_CYC
    {
        type            cyclic;
        value           uniform 0.67;
    }
    INOUT_CYC
    {
        type            cyclic;
        value           uniform 0.67; 
    }
}
