FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1.0; 

boundaryField
{
    WALL_TOP
    {
        type            groovyBC;
        valueExpression "<valueExpressionHere>";
        value           uniform 135.008;
    }
    WALL_BOTTOM
    {
        type            groovyBC;
		valueExpression "<valueExpressionHere>";
        value           uniform 135.008;
    }
    SIDE_CYC
    {
        type            cyclic;
        value           uniform 1.0;
    }
    INOUT_CYC
    {
        type            cyclic;
        value           uniform 1.0; 
    }
}
